{
  "author": {
    "name": "netop://ウエハ",
    "url": "https://webb.page"
  },
  "dependencies": {
    "@webb/console": "^2020.3.29"
  },
  "description": "Pretty good email validation",
  "devDependencies": {
    "@webb/config-prettier": "^2020.3.29",
    "@webb/test": "^2020.3.29",
    "chronver": "^2020.3.9",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "updates": "^10.2.6"
  },
  "homepage": "https://code.webb.page/Webb/validate-email",
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run pretty && npm run increment && git add -A :/"
    }
  },
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@webb/validate-email",
  "prettier": "@webb/config-prettier",
  "repository": {
    "type": "git",
    "url": "https://code.webb.page/Webb/validate-email"
  },
  "scripts": {
    "build": "npm run cleanup && tsc",
    "cleanup": "del './dist'",
    "increment": "chronver --increment package",
    "pretty": "prettier --write 'dist/**/*.js' 'dist/**/*.ts'",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./",
    "test:typescript": "tsc",
    "test:assert": "tsc && ts-node test/index.ts"
  },
  "types": "dist/index.d.ts",
  "version": "2020.03.29"
}
